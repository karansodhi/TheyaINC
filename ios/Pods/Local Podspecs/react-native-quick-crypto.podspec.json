{
  "name": "react-native-quick-crypto",
  "version": "0.5.0",
  "summary": "A fast implementation of Node's `crypto` module written in C/C++ JSI",
  "homepage": "https://github.com/margelo/react-native-quick-crypto#readme",
  "license": "MIT",
  "authors": "Szymon Kapa≈Ça (szymon20000@gmail.com) & Marc Rousavy <me@mrousavy.com> (https://github.com/mrousavy)",
  "platforms": {
    "ios": "11.0",
    "tvos": "12.0",
    "osx": "10.14"
  },
  "source": {
    "git": "https://github.com/mrousavy/react-native-quick-crypto.git",
    "tag": "0.5.0"
  },
  "source_files": [
    "ios/**/*.{h,m,mm}",
    "cpp/**/*.{h,c,cpp}",
    "ios/QuickCryptoModule.h"
  ],
  "pod_target_xcconfig": {
    "USE_HEADERMAP": "YES",
    "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/ReactCommon\" \"$(PODS_TARGET_SRCROOT)\"  \"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/boost-for-react-native\" \"$(PODS_ROOT)/DoubleConversion\" \"$(PODS_ROOT)/Headers/Private/React-Core\" "
  },
  "xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/boost-for-react-native\" \"$(PODS_ROOT)/glog\"  \"${PODS_ROOT}/Headers/Public/React-hermes\" \"${PODS_ROOT}/Headers/Public/hermes-engine\""
  },
  "dependencies": {
    "OpenSSL-Universal": [

    ],
    "React-Core": [

    ],
    "React": [

    ],
    "React-callinvoker": [

    ]
  }
}
